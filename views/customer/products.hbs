                                        <span><a href="#" onclick="return cartAction('add','{{this._id}}')" class="add_cart">+ add to cart</a></span>
                                        <span  class="ml-4"><a href="#" onclick="return wishlistAction('{{this._id}}','{{this.isWished}}','{{../wishlist}}')">
                                            {{#if this.isWished}}
                                            <i id="{{this._id}}" class="fa fa-heart ml-3"></i>
                                            {{else}}
                                            <i id="{{this._id}}" class="ti-heart ml-3 "></i>
