                                        <span class="ml-4"><a href="/auth"><i class="ti-heart ml-3 "></i></a></span>
                                    </div>
                                    
                                    {{/if}}
                                    {{/if}}
                                 
                                    
                                    
                                </div>
                            </div>
                        </div>

                        
                        {{/each}}
                        {{/if}}

                        {{#if filterRender.filterData}}
                        {{#each filterRender.filterData}}
                        <div class="col-lg-4 col-sm-6">
                            <div class="single_product_item">
                                <img src="/uploads/{{this.productPictures.[0].filename}}" alt="">
                                <div class="single_product_text">
                                    <h4>{{this.name}}</h4>
                                    <h3>Rs {{this.price}}</h3>
                                    {{#if this.outOfStock}}
                                    <p style="color:red;">Out of stock</p>
                                    {{else}}
                                    {{#if ../customerLogin}}
                                    <div style="display:flex ;">
                                        <span><a href="#" onclick="return cartAction('add','{{this._id}}')" class="add_cart">+ add to cart</a></span>
                                       <span  class="ml-4"><a href="#" onclick="return wishlistAction('{{this._id}}','{{this.isWished}}','{{../wishlist}}')">
                                            {{#if this.isWished}}
                                            <i id="{{this._id}}" class="fa fa-heart ml-3"></i>
                                            {{else}}
                                            <i id="{{this._id}}" class="ti-heart ml-3 "></i>
                                            {{/if}}
                                            </a></span>
                                    </div>
                                    {{!-- <button onclick="wishlistAction('add','{{this._id}}')">Add to wishlist</button> --}}
                                    {{!-- <a href="#" onclick="return cartAction('add','{{this._id}}')" class="add_cart">+ add to cart</a><a href=""><i class="ti-heart"></i></a> --}}
                                    {{else}}
                                    
                                    <div style="display:flex ;">
                                        <span><a href="/auth" class="add_cart">+ add to cart</a></span>
                                        <span class="ml-4"><a href="/auth"><i class="ti-heart ml-3 "></i></a></span>
                                    </div>
                                    
                                    {{/if}}
                                    {{/if}}
                                 
                                    
                                    
                                </div>
                            </div>
                        </div>

                        
                        {{/each}}
                        {{/if}}
                        <div class="col-lg-12">
                            <div class="pageination">
                                <nav aria-label="Page navigation example">
                                    <form action="">
                                    <ul class="pagination justify-content-center">
                                        <li class="page-item">
                                            <input type="button" name="pagination" value="back" class="page-link" href="#" aria-label="Previous">
                                                <i class="ti-angle-double-left"></i>
                                            
                                        </li>
                                        
                                        {{!-- <li class="page-item"><a class="page-link" href="#">1</a></li>
                                        <li class="page-item"><a class="page-link" href="#">2</a></li>
                                        <li class="page-item"><a class="page-link" href="#">3</a></li>
                                        <li class="page-item"><a class="page-link" href="#">4</a></li>
                                        <li class="page-item"><a class="page-link" href="#">5</a></li>
                                        <li class="page-item"><a class="page-link" href="#">6</a></li> --}}
                                        <li class="page-item">
                                            <input type="button" name="pagination" class="page-link" value="next" href="#" aria-label="Next">
                                                <i class="ti-angle-double-right"></i>
                                            
                                        </li>
                                    </ul>
                                    </form>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
