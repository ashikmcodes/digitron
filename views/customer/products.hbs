                                            <span class="input-group-text" id="inputGroupPrepend"><i
                                                    class="ti-search"></i></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div id="rlFilter" class="row align-items-center latest_product_inner">
                         {{#if productData}}  
                        {{#each productData}}
                        <div class="col-lg-4 col-sm-6">
                            <div class="single_product_item">
                                <a href="/single-view/{{this._id}}">
                                    <img src="/uploads/{{this.productPictures.[0].filename}}" alt="">
                                </a>
                                
                                <div class="single_product_text">
                                    <a href="/single-view/{{this._id}}">
                                        <h4>{{this.name}}</h4>
                                    </a>
                                    <h3>Rs {{this.price}}</h3>
