    <section class="cat_product_area section_padding">
        <div class="container">
            <div class="row">
                <div class="col-lg-3">
                    <div class="left_sidebar_area">


                    <aside class="left_widgets p_filter_widgets">
                        <form id="filter" action="" method="" >

                            <div class="l_w_title">
                                <h3>Sort By Price</h3>
                            </div>
                            <div class="widgets_inner">
                                <ul class="list">

                                    <li>
                                        <label for="">Low to High</label>
                                        <input value="ascending" name="sorting"  id="" type="radio">
                                    </li>
                                     <li>
                                        <label for="">High to Low</label>
                                        <input value="descending" name="sorting"  id="" type="radio">
                                    </li>
                                    
                                    
                                </ul>
                              
                            </div>

                            <div class="l_w_title">
                                <h3>Category filters</h3>
                            </div>
                            <div class="widgets_inner">
                                <ul class="list">

                                    {{#each categoryData}}
                                    <li>
                                        <label for="categId">{{this._id}}</label>
                                        <input name="categ" value="{{this._id}}" id="{{this._id}}" type="checkbox">
                                    </li>
                                    {{/each}}
                                    <li>
                                        <input hidden checked name="categ" value="empty1" id="categId" type="checkbox">
                                    </li>
                                    <li>
                                        <input hidden checked name="categ" value="empty2" id="categId" type="checkbox">
                                    </li>
                                    
                                </ul>
                              
                            </div>


                            <div class="l_w_title">
                                <h3>Brand filters</h3>
                            </div>
                            <div class="widgets_inner">
                                <ul class="list">
                                    
                                    {{#each brandData}}
                                    <li>
                                        <label for="brandId">{{this._id}}</label>
                                        <input name="brand" value="{{this._id}}" id="{{this._id}}" type="checkbox">
                                    </li>
                                    {{/each}}
                                    <li>
                                        <input hidden checked name="brand" value="empty1" id="brandId" type="checkbox">
                                    </li>
                                    <li>
                                        <input hidden checked name="brand" value="empty2" id="brandId" type="checkbox">
                                    </li>

                                </ul>
                              
                            </div>
                            <div class="l_w_title">
                                <h3>Stock</h3>
                            </div>
                            <div class="widgets_inner">
                                <ul class="list">

                                    <li>
                                        <label for="">Include out of Stock</label>
                                        <input name="stock" value="outOfStock" type="checkbox">
                                    </li>

                                </ul>
                              
                            </div>
                        
                        </form>    
                        </aside>
                        

                        

                   

                    </div>
                </div>
                <div class="col-lg-9">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="product_top_bar d-flex justify-content-between align-items-center">
                                <div class="single_product_menu">
                                    <p><span>10000 </span> Product Found</p>
                                </div>
