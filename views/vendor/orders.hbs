{{#if newOrders}}
{{!-- SHOW ALL CATEGORY  LIST STARTS--}}
        <div class="dash-container mt-5 p-4">
          <h4 class="text-center mb-5">New Orders</h4>
<section >

   <div class="container shadow p-2">
      
        <table class="table " id="placed" >
  <thead>
    <tr>
      
      <th scope="col">INDEX</th>
      <th scope="col">CUSTOMER</th>
      <th scope="col">PRODUCT</th>
      <th scope="col">PAYMENT TYPE</th>
      <th scope="col">AMOUNT</th>
      <th scope="col">QTY</th>
      <th scope="col">STATUS</th>
      <th scope="col">VIEW DETAILS</th>
       
      
    </tr>
  </thead>
  <tbody>
      {{#each newOrders}}
    <tr>
     
      <td>{{inc @index}}</td>
      <td> {{this.customerLookup.fullName}}</td>
      <td> {{this.productLookup.name}}</td>
      <td> {{this.orderLookup.paymentType}}</td>
      <td> {{this.orderLookup.items.payablePrice}}</td>
      <td> {{this.orderLookup.items.purchasedQty}}</td>
      <td>
       <a href="" class="mr-2 btn btn-success" onclick="return orderUpdate('packed','{{this.orderLookup._id}}','{{this.productLookup._id}}')">Mark as packed</a>
      </td>
      <td>
        <a href="/admin/block-cat/{{this._id}}" class="ml-3 btn btn-primary" onclick="return confirm('are you sur to block  {{this.name}}')">View</a>
      </td>
    </tr>
    
    {{/each}}
   
  </tbody>
</table>
   </div>
</section>


<script>
  $(document).ready(function () {


    $('#user_table').wrap('<div id="hide" style="display:none"/>');

    $('#hide').css('display', 'block');
  });


  var table = $('#placed').DataTable();
</script>
        </div>
        
 {{!-- SHOW ALL CATEGORY  LIST ends--}}
{{/if}}


